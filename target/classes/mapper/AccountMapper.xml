<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//www.mybatis.org//DTD Mapper 3.0//EN"
        "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.corewell.study.dao.AccountDao">

    <select id="selectAccount" parameterType="String" resultType="Account">
        select * from account where account=#{account} and del_flag=1
    </select>

</mapper>

