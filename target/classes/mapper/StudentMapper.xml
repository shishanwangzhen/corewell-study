<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//www.mybatis.org//DTD Mapper 3.0//EN"
        "http://www.mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.corewell.study.dao.StudentDao">

    <select id="selectStudent" parameterType="String" resultType="AccountDo">
        select account,password from student where account=#{account} and status=1
    </select>

</mapper>

